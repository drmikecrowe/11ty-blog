{
    "compilerOptions": {
        "target": "ES2022",
        "strict": true,
        "noImplicitAny": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "moduleResolution": "NodeNext",
        "module": "NodeNext",
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "noEmit": true
    }
}